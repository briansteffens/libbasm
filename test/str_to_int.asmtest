extern str_to_int

section .data

    input: db "{{ input }}"
    input_len: equ $-input

section .text

global _start
_start:
    mov rdi, input
    mov rsi, input_len
    call str_to_int

    xor rdi, rdi
    cmp rax, {{ output }}
    je return
    mov rdi, 1

return:
    mov rax, 60
    syscall

